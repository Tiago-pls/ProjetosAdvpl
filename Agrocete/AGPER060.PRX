				If ( cCpoPD $ aCodFol[040,1]+"*"+aCodFol[407,1]+"*"+aCodFol[1338,1]+"*"+aCodFol[1339,1]+"*"+aCodFol[1340,1]+"*"+aCodFol[1341,1] +"*"+aCodFol[1342,1] ) .And. ;
