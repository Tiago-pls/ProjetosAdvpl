	nInssPro	:= 0
	nDifBs13o	:= 0
	nDifBsINSS	:= 0 
	nCSFgts		:= 0
	nCSFgts13	:= 0
	cCAnter		:= '' 
	cFAnter	  	:= ''
	lTemDedMat 	:= .F.
	lContoufunc	:= .F. //-- controla se o funcionario ja foi contado  	
