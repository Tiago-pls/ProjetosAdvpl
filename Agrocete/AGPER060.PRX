			ElseIf cCpoPD == aCodFol[399,1]		//-- Base de INSS Diferenca de Dissidio 13o
				nDifBs13o += nCpoVlr
			ElseIf cCpoPd == aCodfol[943,1]		// Empresa Dissidio
				nEmpresa += nCpoVlr
		   	ElseIf cCpoPd == aCodFol[944,1]		// Terceiros
