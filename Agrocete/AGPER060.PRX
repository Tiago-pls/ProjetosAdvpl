	nT13Tot3o   := 0
	nTotTotV3o  := 0
	nT13TotV3o  := 0
	nFilINSS    := 0
	nGerINSS    := 0
	cTerceiros  := ""
	nTot3oF     := 0
	nFgtsAnt13	:= 0
	aTotaINSSEmp := Array(20)
	aT13aINSSEmp := Array(20)
