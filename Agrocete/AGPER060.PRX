			ElseIf cCpoPd == aCodFol[186,1]  // Sesi
			   	nSesi += nCpoVlr
			ElseIf cCpoPd == aCodFol[187,1]  // Senac
			   	nSenac += nCpoVlr
			ElseIf cCpoPd == aCodFol[188,1]  // Sesc
			   	nSesc += nCpoVlr
			ElseIf cCpoPd == aCodFol[189,1]  // Sebrae
			   	nSebrae += nCpoVlr
			ElseIf cCpoPd == aCodFol[190,1]  // Dpc
