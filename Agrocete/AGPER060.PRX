			DbSelectArea("SRG")
			SRG->( DbSetOrder(1) )
			If SRG->( DbSeek( SRA->(RA_FILIAL + RA_MAT) + AnoMes( MonthSum( dDtRefe, 1 ))))
				VerCodSaq(SRG->RG_FILIAL, SRG->RG_MAT, SRG->RG_DATADEM, , @lMesAnte)
				If lMesAnte
					(cAlias)->( DbSkip() )
