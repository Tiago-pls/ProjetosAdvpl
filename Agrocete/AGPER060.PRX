			ElseIf cCpoPd == aCodFol[191,1]  // Faer
			   	nFaer += nCpoVlr
			ElseIf cCpoPd == aCodFol[192,1]  // Senab
			   	nSenab += nCpoVlr
			ElseIf cCpoPd == aCodFol[193,1]  // Seconc
