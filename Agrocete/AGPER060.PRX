			ElseIf cCpoPD == aCodFol[295,1]  	//Contrib. 0.5% s/Rescisao
				nCSFgts	+= nCpoVlr
			ElseIf cCpoPD == aCodFol[298,1]		//Contrib. 0.5% s/Rem. Folha.
